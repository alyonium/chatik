{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "jsx": "react-jsx",
    "allowImportingTsExtensions": false,
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "api/*": ["./src/api/*"],
      "components/*": ["./src/components/*"],
